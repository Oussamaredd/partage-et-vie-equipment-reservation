{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage",
    "test:component": "vitest run tests/component --coverage",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test tests/e2e/reservation.spec.js",
    "test:e2e:smoke": "playwright test tests/e2e/smoke-real.spec.js",
    "test:e2e:all": "playwright test tests/e2e/reservation.spec.js tests/e2e/smoke-real.spec.js --workers=1",
    "test:all": "npm run test:unit && npm run test:e2e:all"
  },
  "dependencies": {
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^28.1.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
